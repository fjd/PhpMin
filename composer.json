{
	"name": "nitra/php-min",
	"version": "0.0.6",
	"type": "library",
	"description": "Minifying CSS, JS",
	"keywords": ["cssmin","jsminplus","minify"],
	"homepage": "http://code.google.com/p/cssmin/",
	"license": "MIT",
	"authors": [
        {
            "name": "Tino Zijdel",
        	"email": "crisp@tweakers.net",
		    "homepage": "https://raw.github.com/mrclay/minify/master/min/lib/JSMinPlus.php"
        },	
		{
			"name": "Joe Scylla",
			"email": "joe.scylla@gmail.com",
			"homepage": "https://profiles.google.com/joe.scylla"
		}
	],
	"require": {
		"php": ">=5.3",
        "cssmin/cssmin": "*",
        "mrclay/minify": "*"
	},
	"repositories": [
        {
            "type": "package",
            "package": {
                "name": "cssmin/cssmin",
                "version": "3.0.1",
                "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
                "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mrclay/minify",
                "version": "2.1.5",
                "dist": { "url": "http://minify.googlecode.com/files/minify-2.1.5.zip", "type": "zip" },
                "autoload": { "classmap": [ "min/lib/" ] }
            }
        }
    ]
}